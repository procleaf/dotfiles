defaults
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
auth on
logfile ~/.msmtp.log

account qiye
port 25
tls off
tls_starttls off
host smtp.cd.sigma-rt.com
from qiye@sigma-rt.com
auth plain
user qiye
passwordeval gpg -q --for-your-eyes-only --no-tty -d ~/.pass.gpg | awk '/sigma-rt-smtp/ { print $NF }'

account gmail
host smtp.gmail.com
port 465
from yqming@gmail.com
tls_starttls off
auth on
user yqming
passwordeval gpg -q --for-your-eyes-only --no-tty -d ~/.pass.gpg | awk '/yqming@gmail-smtp/ { print $NF }'

